  BdIntegratedFunction[1, 1], 
  tau= 0..delta_t
);
Bd[2, 1]:= int(
  BdIntegratedFunction[2, 1], 
  tau= 0..delta_t
);
eval(Bd);
</Text-field>
</Input>
